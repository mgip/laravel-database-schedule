{
  "name": "mgip/laravel-database-schedule",
  "version": "1.0",
  "description": "Gestion Multi App de Tareas mediante Base de Datos",
  "keywords": ["laravel", "schedule", "task", "jobs"],
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "laravel/framework": "^5.8|6.*|7.*|8.*|9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^5.20",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "authors": [
    {
      "name": "Juan Carlos Nava",
      "email": "tuinformatico@zoho.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "RobersonFaria\\DatabaseSchedule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RobersonFaria\\DatabaseSchedule\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RobersonFaria\\DatabaseSchedule\\DatabaseSchedulingServiceProvider"
      ]
    }
  },
  "scripts": {
    "phpcs": "./vendor/bin/phpcs .",
    "tests": "./vendor/bin/phpunit ./tests"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
